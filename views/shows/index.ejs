<section class="topmedia">
    <br>
<br>
<div> <!--list shows-->
    <ul>
    <% shows.forEach ( show => { %>
        <div class='media-box'><a href="/shows/<%=show.id%>"><img src="<%= show.coverImagePath || "images/missing_show.png"%>"><%=show.name%></a></div>
    <% }) %>
    </ul>
</div>

<p class="add-new">Can't find what you're looking for? <a href="shows/new">Add a new show.</a></p>
<hr>
</section>