<div> <!--list staff-->
    <ul>
    <% staff.forEach ( staff => { %>
        <li class='media-box'><a href="/staff/<%=staff.id%>"><img src="<%= staff.coverImagePath || "images/missing_movie.png"%>"><br><%= staff.name %></a></li>
    <% }) %>
    </ul>
</div>
<p class="add-new">Can't find what you're looking for? <a href="staff/new">Add a new staff member.</a></p>
